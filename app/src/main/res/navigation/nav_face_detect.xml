<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_face_detect"
    app:startDestination="@id/takeFacePhotoFragment">

    <fragment
        android:id="@+id/takeFacePhotoFragment"
        android:name="com.ducpv.module.facedetect.TakeFacePhotoFragment"
        android:label="TakeFacePhoto">
        <action
            android:id="@+id/action_takeFacePhotoFragment_to_previewPhotoFragment"
            app:destination="@id/previewPhotoFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/slide_to_left"
            app:popEnterAnim="@anim/slide_from_left"
            app:popExitAnim="@anim/slide_to_right"
            app:popUpTo="@id/takeFacePhotoFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/previewPhotoFragment"
        android:name="com.ducpv.module.facedetect.PreviewPhotoFragment"
        android:label="PreviewPhoto">
        <argument
            android:name="image"
            app:argType="java.io.File" />
    </fragment>
</navigation>
